{
  "unicast": "127.0.0.1",
  "logging": {
    "level": "debug",
    "console": "true",
    "file": {
      "enable": "false",
      "path": "/var/log/vsomeip.log"
    },
    "dlt": "false",
    "version": {
      "enable": "false"
    },
    "__status_log_interval": "30",
    "__statistics": {
      "interval": "60",
      "min-frequency": "5",
      "max-messages": "10"
    }
  },
  "applications": [
    {
      "name": "hello_service",
      "id": "0x1234",
      "max_dispatchers": "10",
      "max_dispatch_time": "100",
      "threads": "2"
    }
  ],
  "services": [
    {
      "service": "0x6000",
      "instance": "0x0001",
      "unreliable": "31231",
      "reliable": { "port" : "31231", "enable-magic-cookies" : "false" }
    }
  ],
  "routing": "hello_service",
  "service-discovery": {
    "enable": "true",
    "multicast": "239.0.0.1",
    "port": "30490",
    "protocol": "udp",
    "ttl": "60",
    "cyclic_offer_delay" : "10000",
    "max_remote_subscribers": "1"
  }
}
